import java.awt.Color;

public class Spadille extends Card {

	public Spadille()
	{
		super(Color.RED, "This card automatically kills your opponent with one strike but also takes 30 energy to execute", 30, "Spadille");
	}
	
	public void use(Hero hero, Enemies enemy)
	{
		enemy.takeDamage(enemy.getOHealth());
		
		
		
	}
	
	
}
